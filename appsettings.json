{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "RagAgentApi.Agents": "Debug"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  },
  
  "Azure": {
    "OpenAI": {
      "Endpoint": "YOUR_AZURE_OPENAI_ENDPOINT", 
      "Key": "YOUR_AZURE_OPENAI_KEY",
      "EmbeddingDeployment": "text-embedding-ada-002",
      "ChatDeployment": "gpt-35-turbo",
      "ApiVersion": "2024-02-15-preview",
      "MaxTokens": 8192,
      "Temperature": 0.7
    },
    "Search": {
      "Endpoint": "YOUR_AZURE_SEARCH_ENDPOINT",
      "Key": "YOUR_AZURE_SEARCH_KEY",
      "IndexName": "rag-agent-documents"
    },
    "Storage": {
      "ConnectionString": "YOUR_AZURE_STORAGE_CONNECTION_STRING",
      "ContainerName": "rag-documents"
    },
    "DocumentIntelligence": {
      "Endpoint": "YOUR_AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT",
      "Key": "YOUR_AZURE_DOCUMENT_INTELLIGENCE_KEY"
    }
  },
  
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Port=5433;Database=ragagentdb;Username=postgres;Password=YOUR_POSTGRES_PASSWORD"
  },
  
  "RagSettings": {
    "Mode": "hybrid",
    "DefaultChunkSize": 1000,
    "DefaultChunkOverlap": 200,
    "MinChunkSize": 100,
    "MaxChunkSize": 5000,
    "DefaultTopK": 5,
    "MaxTopK": 50,
    "BatchSize": 100,
    "VectorDimensions": 1536,
    "MinimumSearchScore": 0.5
  },
  
  "DemoSettings": {
    "DataSource": "local",
    "LocalDataPath": "demos/",
    "PostgresConnectionString": "Host=localhost;Port=5433;Database=ragagentdb;Username=postgres;Password=YOUR_POSTGRES_PASSWORD",
    "SaveResults": true,
    "ResultRetentionDays": 30
  }
}